{
  "name": "PasteHotkey (Tutorial)",
  "description": "Paste with hotkey",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "static/img/favicon/16.png",
    "32": "static/img/favicon/32.png",
    "48": "static/img/favicon/48.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripting/hotkey.js"
      ]
    }
  ],
  "options_page": "/options/options.html"
}
